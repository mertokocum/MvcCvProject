
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>AdminLTE 3 | Project Add</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/AdminLTE-3.0.4/plugins/fontawesome-free/css/all.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- overlayScrollbars -->
    <link rel="stylesheet" href="~/AdminLTE-3.0.4/dist/css/adminlte.min.css">
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
</head>
<body class="hold-transition sidebar-mini">
    <!-- Site wrapper -->
    <div class="wrapper" font-family: din-round,sans-serif; font-size: 2em;>
        <!-- Navbar -->
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a href="~/Default/Index/" class="brand-link">
                <i class="nav-icon fa fa-home"></i>
                <span class="brand-text font-weight-light">Siteye Git</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user (optional) -->
                <div style="justify-content:center;" class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div style="padding-left:0rem" class="image">
                            <img style="max-width:160px; max-height:284px;" src="https://i.hizliresim.com/na652x0.jpg" class="img-circle elevation-2" alt="User Image">
                        
                    </div>
                </div>

                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <!-- Add icons to the links using the .nav-icon class
                        with font-awesome or any other icon font library -->
                        <li class="nav-item has-treeview">
                            <a href="~/Hakkimda/Index/" class="nav-link">
                                <i class="nav-icon fa fa-info-circle" aria-hidden="true"></i>
                                <p>
                                    Hakkımda
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="~/Egitimler/Index/" class="nav-link">
                                <i class="nav-icon fa fa-university" aria-hidden="true"></i>
                                <p>
                                    Eğitimlerim
                                </p>
                            </a>
                        </li>
                        <li class="nav-item has-treeview">
                            <a href="~/Deneyimlerim/Index/" class="nav-link">
                                <i class="nav-icon fa fa-briefcase" aria-hidden="true"></i>
                                <p>
                                    Deneyimlerim
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="~/Yeteneklerim/Index/" class="nav-link">
                                <i class="nav-icon fa fa-magic" aria-hidden="true"></i>
                                <p>
                                    Yeteneklerim
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="~/Projelerim/Index/" class="nav-link">
                                <i class="nav-icon fa fa-tasks" aria-hidden="true"></i>
                                <p>
                                    Projelerim
                                </p>
                            </a>
                        </li>

                        <li class="nav-item">
                            <a href="~/Sertifikalarim/Index" class="nav-link">
                                <i class="nav-icon fa fa-certificate" aria-hidden="true"></i>
                                <p>
                                    Sertifikalarım
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="~/SosyalMedya/Index/" class="nav-link">
                                <i class="nav-icon fa fa-globe" aria-hidden="true"></i>
                                <p>
                                    Sosyal Medya
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="~/Referanslarim/Index/" class="nav-link">
                                <i class="nav-icon fa fa-users" aria-hidden="true"></i>
                                <p>
                                    Referanslarım
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="~/Iletisim/Index" class="nav-link">
                                <i class="nav-icon fa fa-envelope" aria-hidden="true"></i>
                                <p>
                                    İletişim
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="~/Login/LogOut" class="nav-link">
                                <i class="nav-icon fa fa-times-circle" aria-hidden="true"></i>
                                <p>
                                    Çıkış
                                </p>
                            </a>
                        </li>

                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div style="background: gray-light;" class="content-wrapper">
            <section class="content">
                <div class="row">
                    <div width: 100%;" class="col-md-12">
                        @RenderBody()

                    </div>
                </div>

            </section>
            <!-- /.content -->
        </div>

    </div>
    <!-- ./wrapper -->
    <!-- jQuery -->
    <script src="/AdminLTE-3.0.4/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="/AdminLTE-3.0.4/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="/AdminLTE-3.0.4/dist/js/adminlte.min.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="/AdminLTE-3.0.4/dist/js/demo.js"></script>
</body>
</html>