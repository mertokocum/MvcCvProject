@using MvcCvPrroject.Models.Entity
@model List<tblDeneyimlerim>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<h2>Index</h2>
<br />
<h2>Deneyim Listesi</h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Başlık</th>
        <th>Alt Başlık</th>
        <th>Tarih</th>
        <th>Sil</th>
        <th>Düzenle</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ID</td>
            <td>@item.Baslik</td>
            <td>@item.AltBaslik</td>
            <td>@item.Tarih</td>
            <td><a href="~/Deneyimlerim/DeneyimSil/@item.ID" class="btn btn-danger">Sil</a></td>
            <td><a href="~/Deneyimlerim/DeneyimGetir/@item.ID" class="btn btn-warning">Düzenle</a></td>
        </tr>

    }

</table>
<a href="~/Deneyimlerim/DeneyimEkle/" class="btn btn-primary">Yeni Deneyim Ekle</a>



